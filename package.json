{
  "name": "sphere-node-sdk",
  "description": "Officially supported Node.js SDK library for working with the SPHERE.IO HTTP API, with OAuth2 support.",
  "version": "2.0.0-alpha.13",
  "private": false,
  "homepage": "https://github.com/sphereio/sphere-node-sdk",
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-sdk/issues"
  },
  "license": "MIT",
  "main": "./dist",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "coverage": "npm run lint && ./scripts/version && NODE_ENV=test babel-node $(npm bin)/isparta cover test",
    "dist": "npm run clean && ./scripts/version && babel lib --out-dir dist",
    "lint": "eslint lib test",
    "test": "./scripts/version && NODE_ENV=test babel-tape-runner test | tap-spec",
    "test:ci": "browserify -t babelify -t [ envify --NODE_ENV test ] test | tape-run -b firefox",
    "test:browser": "browserify -t babelify -t [ envify --NODE_ENV test ] test | tape-run -b chrome",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "async": "~1.4.2",
    "babel-runtime": "~5.8.25",
    "error-class": "~1.1.1",
    "isomorphic-fetch": "~2.2.0",
    "jsondiffpatch": "emmenko/jsondiffpatch#patch-webpack",
    "lodash.flatten": "^3.0.2",
    "lodash.foreach": "^3.0.3",
    "lodash.uniq": "^3.2.2"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-tape-runner": "^1.2.0",
    "babelify": "^6.4.0",
    "browserify": "^11.2.0",
    "coveralls": "^2.11.4",
    "envify": "^3.4.0",
    "eslint": "^1.7.3",
    "eslint-config-commercetools": "^0.2.1",
    "isparta": "^3.1.0",
    "sinon": "^1.17.2",
    "tap-spec": "^4.1.0",
    "tape": "^4.2.2",
    "tape-run": "^1.1.0"
  },
  "keywords": [
    "sphere",
    "sphereio",
    "rest",
    "api",
    "sdk",
    "sync",
    "promise",
    "stream",
    "ecommerce"
  ],
  "eslintConfig": {
    "extends": "commercetools"
  }
}
