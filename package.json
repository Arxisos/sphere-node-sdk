{
  "name": "sphere-node-sdk",
  "description": "Officially supported Node.js SDK library for working with the SPHERE.IO HTTP API, with OAuth2 support.",
  "version": "2.0.0-alpha.2",
  "private": false,
  "homepage": "https://github.com/sphereio/sphere-node-sdk",
  "author": "Nicola Molinari <nicola.molinari@commercetools.de>",
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-sdk/issues"
  },
  "license": "MIT",
  "main": "./dist",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "coverage": "npm run lint && ./scripts/version && NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --compilers js:babel/register --recursive",
    "dist": "npm run clean && ./scripts/version && babel lib --out-dir dist",
    "lint": "eslint lib test",
    "test": "./scripts/version && NODE_ENV=test mocha --compilers js:babel/register --recursive",
    "test:watch": "npm run test -- --watch"
  },
  "dependencies": {
    "async": "1.4.2",
    "babel-runtime": "5.8.20",
    "error-class": "1.1.0",
    "isomorphic-fetch": "2.1.1"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "coveralls": "^2.11.4",
    "eslint": "^1.3.1",
    "eslint-config-commercetools": "^0.1.0",
    "expect": "^1.9.0",
    "isparta": "^3.0.4",
    "mocha": "^2.3.0",
    "sinon": "^1.16.1"
  },
  "keywords": [
    "sphere",
    "sphereio",
    "rest",
    "api",
    "sdk",
    "sync",
    "promise",
    "stream",
    "ecommerce"
  ]
}
