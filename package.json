{
  "name": "sphere-node-sdk",
  "description": "Officially supported Node.js SDK library for working with the SPHERE.IO HTTP API, with OAuth2 support.",
  "version": "1.3.3",
  "private": true,
  "homepage": "https://github.com/sphereio/sphere-node-sdk",
  "author": {
    "name": "Nicola Molinari",
    "email": "nicola.molinari@commercetools.de"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/sphereio/sphere-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/sphereio/sphere-node-sdk/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/sphereio/sphere-node-sdk/blob/master/LICENSE-MIT"
    }
  ],
  "main": "./dist",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "clean": "rm -rf dist && mkdir -p dist",
    "dist": "npm run clean && babel lib --out-dir dist",
    "lint": "eslint lib test",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive",
    "test:watch": "npm run test -- --watch",
    "postinstall": "npm run dist"
  },
  "dependencies": {
    "async": "0.9.0",
    "babel": "5.8.21",
    "babel-runtime": "5.8.20",
    "isomorphic-fetch": "2.0.2"
  },
  "devDependencies": {
    "babel-core": "5.8.22",
    "babel-eslint": "3.1.23",
    "chai": "2.3.0",
    "eslint": "0.19.0",
    "mocha": "2.2.4",
    "sinon": "1.14.1",
    "sinon-chai": "2.7.0"
  },
  "keywords": [
    "sphere",
    "sphereio",
    "rest",
    "api",
    "sdk",
    "sync",
    "promise",
    "stream",
    "ecommerce"
  ]
}
